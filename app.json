{
  "expo": {
    "name": "Pugdom",
    "slug": "pugdom",
    "scheme": "pugdom",
    "platforms": ["ios", "android", "web"],
    "ios": {
      "bundleIdentifier": "com.pugdom.pugdom",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["pugdom"]
          }
        ]
      }
    },
    "android": {
      "package": "com.pugdom.pugdom",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "pugdom",
            "host": "oauthredirect"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    }
  }
}
